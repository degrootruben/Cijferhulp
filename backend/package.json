{
  "name": "cijferhulp",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "test:up": "docker-compose -f tests/api-tests/docker-compose-test-database.yml --env-file .env up -d && export NODE_ENV=testing && nodemon",
    "test": "jest",
    "test:down": "docker-compose -f tests/api-tests/docker-compose-test-database.yml --env-file .env down",

    "build": "rimraf ./build && npx -p typescript tsc",
    "start": "set NODE_ENV=production && npm run build && node build/app.js",

    "dev": "docker-compose up -d && export NODE_ENV=development && nodemon",
    "dev:down": "docker-compose down"
  },
  "author": "Ruben de Groot",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/client-sessions": "^0.8.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.5",
    "@types/node-fetch": "^2.5.10",
    "@types/pg": "^8.6.0",
    "@types/uuid": "^8.3.1",
    "jest": "^27.4.2",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "client-sessions": "^0.8.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.6",
    "pg": "^8.7.1",
    "typescript": "^4.3.4",
    "uuid": "^8.3.2",
    "yarn": "^1.22.10"
  },

  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "./src/",
      "./frontend"
    ]
  }
}


